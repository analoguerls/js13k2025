<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="dist\css\**" />
    <Compile Remove="dist\js\**" />
    <Content Remove="dist\css\**" />
    <Content Remove="dist\js\**" />
    <EmbeddedResource Remove="dist\css\**" />
    <EmbeddedResource Remove="dist\js\**" />
    <None Remove="dist\css\**" />
    <None Remove="dist\js\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="wwwroot\css\style.css" />
    <Content Remove="wwwroot\index.html" />
    <Content Remove="wwwroot\js\script.js" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="dist\images\" />
    <Folder Include="src\images\" />
    <Folder Include="wwwroot\images\" />
    <Folder Include="zip\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\css\style.css" />
    <None Include="wwwroot\index.html" />
    <None Include="wwwroot\js\script.js" />
  </ItemGroup>

</Project>
